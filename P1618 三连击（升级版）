#include<iostream>
#include<cstdio>
#include<algorithm>
#include<cstring>
#define LL long long
using namespace std;
int vis[10]={0};
int main(){
int a,b,c;
cin>>a>>b>>c;
int flagg=0;
for(int i=102;i<330;i++){
int n,flag;
memset(vis,0,sizeof(vis));
vis[0]=1;
int num=i;
int num1=num/a*b;
int num2=num/a*c;

if(num*c!=num2*a)continue;
if(num*b!=num1*a)continue;
if(num1*c!=num2*b)continue;
n=3,flag=0;
int t=num;
while(n--){
    if(vis[t%10]){
        flag=1;
        break;
    }
    else{
        vis[t%10]=1;
        t/=10;
    }
}
if(flag) continue;
flag=0;n=3;
t=num1;
 //cout<<num1<<' ';
while(n--){
    if(vis[t%10]){
        flag=1;
        break;
    }
    else{
        vis[t%10]=1;
        t/=10;
    }
}
if(flag) continue;
flag=0;n=3;
t=num2;
 //cout<<num2<<endl;
while(n--){
    if(vis[t%10]){
        flag=1;
        break;
    }
    else{
        vis[t%10]=1;
        t/=10;
    }
}
if(flag) continue;
printf("%d %d %d\n",num,num1,num2);
flagg=1;
}
if(!flagg)cout<<"No!!!"<<endl;
return 0;
}
